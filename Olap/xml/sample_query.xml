<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE schema SYSTEM "query.dtd" >
<query classifier="comp1" measure="q" aggregate="COUNT">

	<composition name="comp1" leftoperand="proj1" rightoperand="pair1"/>

	<pairing name="pair1" leftoperand="comp2" rightoperand="comp3"/>
	
	<composition name="comp2" leftoperand="f1" rightoperand="f"/>
	
	<composition name="comp3" leftoperand="f2" rightoperand="f"/>
	
	<function name="f"/>
	
	<function name="f1"/>
	
	<function name="f2"/>
	
	<function name="q"/>	
	
	<projection name="proj1">
		<domain>
			<attribute name="Supplier"/>
			<attribute name="Category"/>
		</domain>
		<range>
			<attribute name="Supplier"/>
		</range>
	</projection>
	
</query>